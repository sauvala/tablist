{
  "name": "Tablist",
  "description": "Easily switch between your tabs with your keyboard",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "omnibox": {
    "keyword" : "b"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}